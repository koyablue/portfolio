{
  "extends": "next/core-web-vitals",
  "rules": {
    "eol-last": "error",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreTemplateLiterals": true,
        "ignoreUrls": true,
        "ignoreStrings": true
      }
    ],
    "object-curly-spacing": ["error", "always"]
  }
}
